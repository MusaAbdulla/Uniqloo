@model SlideCreateVM
@{
    ViewData["Title"] = "Create";
}
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Slider Create</h6>
    </div>
    <div class="card-body">
<form method="Post">
   
    <div class="form-group">
        <label asp-for="Title"></label>
        <input class="form-control"asp-for="Title">
        <span class="text-danger" asp-validation-for="Title"></span>
    </div>
    <div class="form-group">
        <label asp-for="Subtitle"></label>
        <input class="form-control"asp-for="Subtitle">
        <span class="text-danger" asp-validation-for="Subtitle"></span>
    </div>
    <div class="form-group">
                <label asp-for="Link"></label>
                <input class="form-control" asp-for="Link">
                <span class="text-danger" asp-validation-for="Link"></span>
    </div>
            <div class="form-group">
                <label asp-for="file"></label>
                <input class="form-control" asp-for="file">
                <span class="text-danger" asp-validation-for="file"></span>
            </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>
</div>
